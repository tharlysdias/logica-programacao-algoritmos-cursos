algoritmo "Enquanto I"

var
   i: inteiro
inicio

      i <- 0
      Escreval("Inicio")
      enquanto i < 100 faca
               Escreval(i)
               i <- i + 1
               
               se i = 50 entao
                  Escreval("O i vale 50 meus caros")
                  interrompa
               fimse
      fimenquanto
      Escreval("Fim")

fimalgoritmo
